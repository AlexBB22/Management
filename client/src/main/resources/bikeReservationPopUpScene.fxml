<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.tudelft.oopp.controllers.BikeReservationPopUpSceneController">
   <children>
        <HBox alignment="CENTER">
            <children>
                <Text strokeType="CENTERED" strokeWidth="0.0" text="   Please confirm your reservation by clicking &quot;Reserve&quot;" wrappingWidth="300" />
            </children>
        </HBox>
      <HBox alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Building:" />
         </children>
          <Text fx:id="building" layoutX="10.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      </HBox>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="189.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Day:" />
         </children>
          <Text fx:id="day" layoutX="10.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      </HBox>
      <Button mnemonicParsing="false" onAction="#makeNewBikeReservation" text="Reserve" />
   </children>
</VBox>
