<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.tudelft.oopp.controllers.BikeReservationPopUpSceneController">
   <children>
        <HBox alignment="CENTER" prefHeight="86.0" prefWidth="300.0">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="These are the details of your new reservation:" textAlignment="CENTER" wrappingWidth="149.98046875">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Text>
            </children>
        </HBox>

      <HBox alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Building:">
                <HBox.margin>
                    <Insets right="10.0" />
                </HBox.margin>
            </Text>
         </children>
          <Text fx:id="building" layoutX="10.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0">
              <HBox.margin>
                  <Insets left="20.0" />
              </HBox.margin>
          </Text>
      </HBox>

      <HBox alignment="CENTER" layoutX="10.0" layoutY="189.0">
         <children>
             <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Day:">
                 <HBox.margin>
                     <Insets right="10.0" />
                 </HBox.margin>
             </Text>

         </children>
          <Text fx:id="day" layoutX="10.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0">
              <HBox.margin>
                  <Insets left="20.0" />
              </HBox.margin>
          </Text>
      </HBox>

      <HBox alignment="CENTER">
         <children>
            <Button fx:id="cancelBikeButton" cancelButton="true" mnemonicParsing="false" onMouseClicked="#backBtnHandler" text="Cancel" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="67.20001530647278" />
            <Button fx:id="reserveButton" mnemonicParsing="false" onAction="#makeNewBikeReservation" text="Reserve" />
         </children>
      </HBox>
   </children>
</VBox>
